generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Event {
  id Int @id @default(autoincrement())
  number String
  title String
  body String?
  dateTime String
  timeZone String
  utcTime DateTime  
}

model Test {
  id Int @id @default(autoincrement())
  currentDate String
  startDate String
  ISOStart String
  endDate String
  endUTC String
  ISOEnd String
}